<script>
    let audio;
    function playAudio(name, loop = false) {
        if(audio) {
            audio.pause();
            audio.currentTime = 0;
            audio = null;
            return;
        }

        audio = new Audio("/audios/" + name + ".mp3");
        audio.loop = loop;
        audio.play();
    }
</script>

